-- Scripts:

local function GQHVC_fake_script() -- SwagCrash.LocalScript 
	local script = Instance.new('LocalScript', SwagCrash)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent["Crash Tab"].Visible = true
	end)
	
end
coroutine.wrap(GQHVC_fake_script)()
local function TYBVF_fake_script() -- Drop.LocalScript 
	local script = Instance.new('LocalScript', Drop)

	Drops = false
	player = game.Players.LocalPlayer
	MainEvent = game:GetService('ReplicatedStorage').MainEvent
	
	script.Parent.MouseButton1Click:Connect(function()	
		Drops = true
		while Drops == true do
			wait(0.1)
			local DropAm = 10000
			if player.DataFolder.Currency.Value > 10000 then
				DropAm = '10000'
			else
				DropAm = tostring(player.DataFolder.Currency.Value)
			end
			local args = {
				'DropMoney',
				DropAm
			}
			MainEvent:FireServer(unpack(args))
		end
	end)
	script.Parent.Parent.undrop.MouseButton1Click:Connect(function()	
		if Drops == true then
			Drops = false
		end
	end)
	
end
coroutine.wrap(TYBVF_fake_script)()
